{
    "name": "jivesh/laravel-slack",
    "description": "A minimalist laravel integration for Slack",
    "type": "library",
    "keywords": ["slack", "laravel"],
    "homepage": "http://github.com/jiveshsg/laravel-slack",
    "require": {
        "guzzlehttp/guzzle": "^6.0"
    },
    "license": "MIT",
    "authors": [{
        "name": "Jivesh S. Gahlawat",
        "email": "jiveshsg@gmail.com"
    }],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Gahlawat\\": "src/Gahlawat"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gahlawat\\Slack\\SlackServiceProvider"
            ],
            "aliases": {
                "Slack": "Gahlawat\\Slack\\Facade\\Slack"
            }
        }
    }
}
